{
  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_SQL_SERVER;Database=YOUR_DB;User Id=YOUR_USER;Password=YOUR_PASS;TrustServerCertificate=True;"
  },
  "Jwt": {
    "Key": "REPLACE_WITH_A_VERY_LONG_SECRET_KEY_AT_LEAST_32_CHARS",
    "Issuer": "YourApp",
    "Audience": "YourAppClients",
    "AccessTokenMinutes": 10,
    "RefreshTokenMinutes": 60
  },
  "Redis": {
    "Configuration": "localhost:6379"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
